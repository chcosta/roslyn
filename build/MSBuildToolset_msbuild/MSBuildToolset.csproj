<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>RoslynRestore</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>MSBuildToolset</PackageId>
    <RuntimeIdentifier>fedora.25-x64</RuntimeIdentifier>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="csc;vbc">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Build" Version="15.1.0-preview-000458-02" />
    <PackageReference Include="Microsoft.Build.Runtime" Version="15.1.0-preview-000458-02" />
    <PackageReference Include="Microsoft.Build.Tasks.Core" Version="15.1.0-preview-000458-02" />
    <PackageReference Include="Microsoft.Net.Compilers.netcore" Version="2.0.0-rc2-61102-09" />
    <PackageReference Include="Microsoft.NuGet.Build.Tasks" Version="0.1.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Build.Tasks" Version="2.0.0-rc2-61102-09" />
    <PackageReference Include="NetStandard.Library" Version="2.0.0-beta-25003-02" />
    <PackageReference Include="Microsoft.Portable.Targets" Version="0.1.2-dev" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
    <PackageReference Include="NuGet.CommandLine.XPlat" Version="3.5.0-beta2-1484" />
    <PackageReference Include="System.ValueTuple" Version="4.3.0" />
    <PackageReference Include="Microsoft.NETCore.TestHost" Version="2.0.0-beta-25014-04" />
    <PackageReference Include="Microsoft.Private.CoreFx.NETCoreApp" Version="4.4.0-beta-24919-06" />
    <PackageReference Include="Microsoft.NETCore.Runtime.CoreCLR" Version="2.0.0-beta-25014-04" />
  </ItemGroup>
</Project>